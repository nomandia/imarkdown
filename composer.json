{
    "name": "nomandia/imarkdown",
    "description": "Simply markdown toolkit",
    "keywords": ["laravel", "markdown", "nomandia"],
    "license": "MIT",
    "authors": [
    {
        "name": "Nomandia",
        "email": "nomandia@qq.com"
    }],
    "require":
    {
        "php": "^7.1.0",
        "laravel/framework": "~5.5",
        "nomandia/iimage": "dev-master",
        "erusev/parsedown": "^1.7",
        "league/html-to-markdown": "^4.6",
        "mews/purifier": "^2.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Nomandia\\IMarkdown\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra":
    {
        "laravel":
        {
            "providers": [
                "Nomandia\\IMarkdown\\IMarkdownProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
